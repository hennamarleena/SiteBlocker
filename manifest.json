{
    "manifest_version": 3,
    "name": "DistractionApp",
    "description": "Blocking distracting websites",
    "version": "0.0.1",
    "permissions": ["activeTab", "storage", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["background.js"]
        }
    ] 
}